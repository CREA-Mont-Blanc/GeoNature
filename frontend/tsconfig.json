{
  "compileOnSave": false,
  "compilerOptions": {
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "target": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "types":[
    ],
    "baseUrl": "./",
    "paths": {
      "@geonature_common/*" : ["src/app/GN2CommonModule/*"],
      "@geonature/*" : ["src/app/*"],
      "@geonature_config/*" : ["src/conf/*"],
      "@librairies/*" : ["./node_modules/*"]
    }
  }
}
